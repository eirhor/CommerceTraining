@using EPiServer.Core

@model CommerceFundamentalsWeb.Models.ViewModels.NavigationViewModel

<b>@Html.ContentLink(ContentReference.StartPage)</b>
@foreach (var page in Model.CmsItems)
{
    if (page.VisibleInMenu)
    {
        <b>
            -:-
            @Html.ContentLink(page)
        </b>
    }
}

@foreach (var commercePage in Model.CommerceItems)
{
    <b>
        -:-
        @Html.ContentLink(commercePage)
    </b>
}