@model CommerceFundamentalsWeb.SupportingClasses.NodeEntryCombo

<h2>Nodes</h2>
<ul>
    @foreach (var item in Model.Nodes)
    {
        <li><a href="@item.Url">@item.Name</a><img src="@item.ImageThumbUrl" /></li>
    }
</ul>

<h2>Entries</h2>
<ul>
    @foreach (var item in Model.Entries)
    {
        <li><a href="@item.Url">@item.Name</a><img src="@item.ImageThumbUrl" /></li>
    }
</ul>